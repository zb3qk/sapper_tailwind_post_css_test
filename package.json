{
  "name": "sapper-postcss-template",
  "description": "A template that includes Sapper for Svelte and PostCSS preprocessing with Tailwind CSS",
  "keywords": [
    "sapper",
    "eslint",
    "postcss",
    "purgecss",
    "svelte",
    "tailwindcss",
    "cssnano"
  ],
  "homepage": "https://github.com/babichjacob/sapper-postcss-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/babichjacob/sapper-postcss-template.git"
  },
  "license": "MIT",
  "version": "2020.12.12",
  "scripts": {
    "eslint": "eslint",
    "eslint:fix": "eslint --fix ./*.js ./src/*.js ./src/routes/**/*.svelte ./src/components/**/*.svelte",
    "dev": "sapper dev --ext \".svelte .md\"",
    "build": "cross-env NODE_ENV=production sapper build --legacy --ext \".svelte .md\"",
    "prod": "npm run build",
    "export": "cross-env NODE_ENV=production sapper export --legacy --ext \".svelte .md\"",
    "start": "node __sapper__/build"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.3.1",
    "@untemps/svelte-readotron": "^1.2.0",
    "compression": "^1.7.4",
    "express": "^4.17.3",
    "mathjax": "^3.1.2",
    "sirv": "^1.0.10",
    "smelte": "^1.0.18",
    "svelte-media": "0.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@sveltejs/svelte-scroller": "^2.0.6",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "front-matter": "^4.0.2",
    "highlight.js": "^10.5.0",
    "kleur": "^4.1.3",
    "mdsvex": "^0.8.9",
    "postcss": "^8.2.1",
    "postcss-import": "^13.0.0",
    "rollup": "^2.34.2",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "svelte": "^3.31.0",
    "svelte-fa": "^2.2.0",
    "svelte-image": "^0.2.9",
    "svelte-preprocess": "^4.6.1",
    "svelte-preprocess-markdown": "^2.7.3",
    "tailwindcss": "^2.0.2",
    "unsplash-js": "^7.0.2"
  }
}
